
@{
    ViewBag.Title = "Create";
}

<h2>New Deck</h2>
<form action="/Deck/Create" method="POST">
    <div>
        <label>Deck Name:</label> <input type="text" name="DeckName" />
    </div>
    <div>
        <label>User ID:</label> <input type="text" name="UserId" />
    </div>
    <div>
        <label>Deck List:</label> <input type="text" name="DeckList" />
    </div>
    <div>
        <input type="submit" value="Add Deck"/>
    </div>
</form>

